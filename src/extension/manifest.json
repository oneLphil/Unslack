{
  "manifest_version": 2,
  "name": "Improve Me",
  "version": "0.1",
  "background": {
    "scripts": ["./chrome-js/background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["./charts/jquery-3.3.1.min.js", "./chrome-js/content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "./charts/pac.png",
    "default_popup": "popup.html"
  }
}
